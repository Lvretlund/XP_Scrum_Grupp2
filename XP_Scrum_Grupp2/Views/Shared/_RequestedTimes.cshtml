@model IEnumerable<XP_Scrum_Grupp2.Models.MeetingTimes>

    @foreach (var time in Model)
    {
        using (Html.BeginForm("ChooseTime", "Meeting", new { Id = time.Id, status = time.ChosenTime }, FormMethod.Post))
        {
        <ul class="list-group">
            <li class="list-group-item">
                <p>@time.Time </p> @Html.CheckBoxFor(m => time.ChosenTime)
            </li>
            </ul>    
        }
    }
